{
  "name": "chrome-cocopy",
  "version": "0.4.0",
  "private": true,
  "repository": "ssh://git@github.com/pokutuna/chrome-cocopy.git",
  "license": "MIT",
  "author": "pokutuna <mail@pokutuna.com>",
  "scripts": {
    "build": "webpack",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "prepare": "husky install",
    "test": "jest",
    "posttest": "yarn run check",
    "test-watch": "jest --watch",
    "update-gallery": "ts-node ./gallery/generate.ts",
    "validator": "ajv compile -s ./src/lib/function.schema.json -o ./src/lib/function.ajv.js",
    "watch": "webpack --watch",
    "zip": "yarn build && zip -r build-$(cat build/manifest.json | jq -r '.version').zip ./build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "gts fix"
    ]
  },
  "resolutions": {
    "@types/react": "18.3.18"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.7.2",
    "@fortawesome/free-solid-svg-icons": "6.7.2",
    "@fortawesome/react-fontawesome": "0.2.2",
    "ajv": "8.17.1",
    "chroma-js": "2.6.0",
    "jsx-runtime": "1.2.0",
    "lodash.debounce": "4.0.8",
    "mustache": "4.2.0",
    "prismjs": "1.30.0",
    "react": "18.3.1",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.30.0",
    "react-simple-code-editor": "0.14.1",
    "styled-components": "5.3.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "13.4.0",
    "@types/chroma-js": "2.4.5",
    "@types/chrome": "0.0.309",
    "@types/jest": "27.5.2",
    "@types/js-yaml": "4.0.9",
    "@types/lodash.debounce": "4.0.9",
    "@types/mustache": "4.2.5",
    "@types/node": "16.18.126",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.34",
    "ajv-cli": "5.0.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.11.0",
    "eslint-plugin-jest": "26.9.0",
    "eslint-plugin-react": "7.37.4",
    "gts": "3.1.1",
    "html-webpack-plugin": "5.6.3",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "28.1.3",
    "jest-chrome": "0.8.0",
    "jest-environment-jsdom": "28.1.3",
    "jest-styled-components": "7.2.0",
    "js-yaml": "4.1.0",
    "license-webpack-plugin": "4.0.2",
    "lint-staged": "12.5.0",
    "style-loader": "3.3.4",
    "ts-jest": "28.0.8",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "typescript": "4.9.5",
    "webpack": "5.98.0",
    "webpack-cli": "4.10.0"
  },
  "engines": {
    "node": ">=16"
  }
}
