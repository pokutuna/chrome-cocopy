{
  "name": "chrome-cocopy",
  "version": "0.1.1",
  "private": true,
  "repository": "ssh://git@github.com/pokutuna/chrome-cocopy.git",
  "license": "MIT",
  "author": "pokutuna <mail@pokutuna.com>",
  "scripts": {
    "build": "webpack",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "test": "jest",
    "posttest": "yarn run check",
    "test-watch": "jest --watch",
    "update-gallery": "ts-node ./gallery/generate.ts",
    "validator": "ajv compile -s ./src/lib/function.schema.json -o ./src/lib/function.ajv.js",
    "watch": "webpack --watch",
    "zip": "yarn build && zip -r build-$(cat build/manifest.json | jq -r '.version').zip ./build",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "gts fix"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "ajv": "8.6.0",
    "chroma-js": "2.1.2",
    "lodash.debounce": "4.0.8",
    "mustache": "4.2.0",
    "preact": "10.5.13",
    "preact-jsx-runtime": "^1.2.0",
    "prismjs": "1.23.0",
    "react-dnd": "14.0.2",
    "react-dnd-html5-backend": "14.0.0",
    "react-router-dom": "5.2.0",
    "react-simple-code-editor": "0.11.0",
    "styled-components": "5.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/preact": "2.0.1",
    "@types/chroma-js": "2.1.3",
    "@types/chrome": "0.0.145",
    "@types/jest": "26.0.23",
    "@types/js-yaml": "4.0.1",
    "@types/lodash.debounce": "4.0.6",
    "@types/mustache": "4.1.1",
    "@types/node": "14.17.4",
    "@types/react-router-dom": "5.1.7",
    "@types/styled-components": "5.1.10",
    "ajv-cli": "5.0.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "5.2.6",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-react": "7.24.0",
    "gts": "3.1.0",
    "html-webpack-plugin": "5.3.2",
    "husky": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.5",
    "jest-chrome": "0.7.1",
    "jest-styled-components": "7.0.4",
    "js-yaml": "4.1.0",
    "license-webpack-plugin": "2.3.19",
    "lint-staged": "11.0.0",
    "style-loader": "3.0.0",
    "ts-jest": "27.0.3",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "typescript": "4.3.4",
    "webpack": "5.40.0",
    "webpack-cli": "4.7.2"
  },
  "engines": {
    "node": ">=12"
  }
}
