{
  "name": "chrome-cocopy",
  "version": "0.3.0",
  "private": true,
  "repository": "ssh://git@github.com/pokutuna/chrome-cocopy.git",
  "license": "MIT",
  "author": "pokutuna <mail@pokutuna.com>",
  "scripts": {
    "build": "webpack",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "test": "jest",
    "posttest": "yarn run check",
    "test-watch": "jest --watch",
    "update-gallery": "ts-node ./gallery/generate.ts",
    "validator": "ajv compile -s ./src/lib/function.schema.json -o ./src/lib/function.ajv.js",
    "watch": "webpack --watch",
    "zip": "yarn build && zip -r build-$(cat build/manifest.json | jq -r '.version').zip ./build",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "gts fix"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.18",
    "ajv": "8.11.0",
    "chroma-js": "2.4.2",
    "lodash.debounce": "4.0.8",
    "mustache": "4.2.0",
    "preact": "10.7.0",
    "preact-jsx-runtime": "1.2.0",
    "prismjs": "1.27.0",
    "react-dnd": "14.0.5",
    "react-dnd-html5-backend": "14.1.0",
    "react-router-dom": "5.3.0",
    "react-simple-code-editor": "0.11.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.3",
    "@testing-library/preact": "2.0.1",
    "@types/chroma-js": "2.1.3",
    "@types/chrome": "0.0.180",
    "@types/jest": "27.4.1",
    "@types/js-yaml": "4.0.5",
    "@types/lodash.debounce": "4.0.6",
    "@types/mustache": "4.1.2",
    "@types/node": "14.18.12",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.24",
    "ajv-cli": "5.0.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "6.7.1",
    "eslint-plugin-jest": "26.1.3",
    "eslint-plugin-react": "7.29.4",
    "gts": "3.1.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "jest-chrome": "0.7.2",
    "jest-styled-components": "7.0.8",
    "js-yaml": "4.1.0",
    "license-webpack-plugin": "3.0.0",
    "lint-staged": "12.3.7",
    "style-loader": "3.3.1",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "webpack": "5.71.0",
    "webpack-cli": "4.9.2"
  },
  "engines": {
    "node": ">=12"
  }
}
