{
  "name": "chrome-cocopy",
  "version": "0.1.0",
  "private": true,
  "repository": "ssh://git@github.com/pokutuna/chrome-cocopy.git",
  "license": "MIT",
  "author": "pokutuna <mail@pokutuna.com>",
  "scripts": {
    "build": "webpack",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "test": "jest",
    "posttest": "yarn run check",
    "update-gallery": "ts-node ./gallery/generate.ts",
    "validator": "ajv compile -s ./src/lib/function.schema.json -o ./src/lib/function.ajv.js",
    "watch": "webpack --watch",
    "zip": "yarn build && zip -r build-$(cat build/manifest.json | jq -r '.version').zip ./build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "gts fix"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.13",
    "ajv": "6.12.6",
    "chroma-js": "2.1.0",
    "lodash.debounce": "4.0.8",
    "mustache": "4.1.0",
    "preact": "10.5.7",
    "prismjs": "1.22.0",
    "react-dnd": "11.1.3",
    "react-dnd-html5-backend": "11.1.3",
    "react-router-dom": "5.2.0",
    "react-simple-code-editor": "0.11.0",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.6",
    "@testing-library/preact": "2.0.1",
    "@types/chroma-js": "2.1.2",
    "@types/chrome": "0.0.127",
    "@types/jest": "26.0.19",
    "@types/js-yaml": "3.12.5",
    "@types/lodash.debounce": "4.0.6",
    "@types/mustache": "4.1.0",
    "@types/node": "13.13.36",
    "@types/react-router-dom": "5.1.6",
    "@types/styled-components": "5.1.5",
    "ajv-cli": "3.3.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.4.0",
    "css-loader": "5.0.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-react": "7.21.5",
    "gts": "3.0.3",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-chrome": "0.7.0",
    "jest-styled-components": "7.0.3",
    "js-yaml": "3.14.1",
    "license-webpack-plugin": "2.3.2",
    "lint-staged": "10.5.3",
    "style-loader": "2.0.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.12",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "webpack": "5.10.1",
    "webpack-cli": "4.2.0"
  },
  "engines": {
    "node": ">=12"
  }
}
